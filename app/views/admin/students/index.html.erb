<p id="notice"><%= notice %></p>

<h1>Listing Students</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Middle name</th>
      <th>Last name</th>
      <th>Country</th>
      <th>Bio</th>
      <th>Class year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_students.each do |admin_student| %>
      <tr>
        <td><%= admin_student.first_name %></td>
        <td><%= admin_student.middle_name %></td>
        <td><%= admin_student.last_name %></td>
        <td><%= admin_student.country %></td>
        <td><%= admin_student.bio %></td>
        <td><%= admin_student.class_year %></td>
        <td><%= link_to 'Show', admin_student_path(admin_student) %></td>
        <td><%= link_to 'Edit', edit_admin_student_path(admin_student) %></td>
        <td><%= link_to 'Destroy', admin_student_path(admin_student), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student', new_admin_student_path %>
